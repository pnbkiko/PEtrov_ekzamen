<Window x:Class="PEtrov_ekzamen.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PEtrov_ekzamen"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" MaxHeight="450" MinHeight="450" MaxWidth="800" MinWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Frame Grid.Row="1" x:Name="MyFrame"></Frame>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <Button Content="1" Click="Button_Click"></Button>
            <Button Content="2" Click="Button_Click_1"></Button>
            <Button Content="3" Click="Button_Click_2"></Button>
        </StackPanel>
    </Grid>
</Window>
